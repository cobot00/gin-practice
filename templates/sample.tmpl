<html>
  <link rel="stylesheet" href="css/application.css">

  <h1>
    {{ .title }}
  </h1>

  <ul>
    {{range .samples}}
    <li class="flex-container">
      <div style="width: 150px">
        <b>Id:</b> {{.ID}}
      </div>
      <div style="width: 150px">
        <b>IntValue:</b> {{.IntValue}}
      </div>
      <div style="width: 150px">
        <b>RealValue:</b> {{.RealValue}}
      </div>
      <div style="width: 150px">
        <b>StringValue:</b> {{.StringValue}}
      </div>
    </li>
    {{end}}
  </ul>
</html>
